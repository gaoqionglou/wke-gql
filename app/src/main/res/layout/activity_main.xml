<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.wke.gql.MainActivity">

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/demo1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toDemo1"
        android:text="Dagger2 + MVP + Retrofit2"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/demo2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toDemo2"
        android:text="Dagger2 + MVP+ Retrofit2 + RxJava2"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/layoutdemo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toLayoutDemo"
        android:text="[仿B站]toolbar+折叠菜单栏"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/custom_behavior_layout_demo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toCustomBehaviorLayoutDemo"
        android:text="自定义Behavior"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/collapsingdemo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toCollapsingDemo"
        android:text="collapsingToolbAR Demo"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/flowlayoutdemo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toFlowlayoutDemo"
        android:text="流式布局"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/customtabdemo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toCustomTabDemo"
        android:text="[仿QQ动漫]自定义tab"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/shopviewdemo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toShopViewdemo"
        android:text="购物车效果"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/citywidgetdemo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toCityWidgetdemo"
        android:text="城市控件-demo1"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/citywidgetdemo2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="toCityWidgetdemo2"
        android:text="城市控件-demo2"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</LinearLayout>
